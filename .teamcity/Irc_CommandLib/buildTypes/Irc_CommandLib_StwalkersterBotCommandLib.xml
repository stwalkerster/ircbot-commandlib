<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="ee9edbe4-8fcf-4795-9e43-62fd19280782" paused="true" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2020.1/project-config.xsd">
  <name>Stwalkerster.Bot.CommandLib</name>
  <description>IRC Bot Command Library</description>
  <settings ref="DotNetCoreLibrary">
    <parameters>
      <param name="extraParameters" value="" />
      <param name="nunitRunner" value="3.12.0" />
      <param name="primaryProjectName" value="Stwalkerster.Bot.CommandLib" />
      <param name="solutionName" value="CommandLib" />
      <param name="version" value="14.0" spec="text display='normal' description='Two or three-part version number. The build counter will be appended to the end.' validationMode='any'" />
      <param name="versiontag" value="-beta" spec="text description='e.g. &quot;-alpha&quot;' validationMode='any' display='normal'" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_15" name="Run Tests" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="mono" />
          <param name="command.parameters" value="%pathPrefix%packages/NUnit.ConsoleRunner.%nunitRunner%/tools/nunit3-console.exe %pathPrefix%%testProjectName%/bin/Release/%testProjectName%.dll --teamcity" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_16" name="" type="jb.nuget.pack">
        <parameters>
          <param name="nuget.pack.as.artifact" value="true" />
          <param name="nuget.pack.commandline" value="-MsbuildPath /usr/lib/mono/msbuild/15.0/bin/" />
          <param name="nuget.pack.output.clean" value="true" />
          <param name="nuget.pack.output.directory" value="output/" />
          <param name="nuget.pack.project.dir" value="as_is" />
          <param name="nuget.pack.properties" value="Configuration=Release" />
          <param name="nuget.pack.specFile" value="%pathPrefix%%primaryProjectName%/%primaryProjectName%.csproj" />
          <param name="nuget.path" value="%teamcity.tool.NuGet.CommandLine.DEFAULT%" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="Irc_BotCommandlib" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="JetBrains.AssemblyInfo" type="JetBrains.AssemblyInfo">
        <parameters>
          <param name="assembly-format" value="%version%.0.0" />
          <param name="file-format" value="%version%.%build.counter%" />
          <param name="info-format" value="%version%.%build.counter%%versiontag%" />
          <param name="patch-global-assembly-info" value="true" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

